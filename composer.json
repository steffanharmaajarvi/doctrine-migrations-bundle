{
    "name": "steffanharmaajarvi/clickhouse-migrations-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ClickhouseMigrationsBundle",
    "keywords": ["Clickhouse", "Migrations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Steffan Kharmaaiarvi",
            "email": "steffankharmaaiarvi@gmail.com"
        },
        {
            "name": "Aleksandr Konnov",
            "email": "a.konnov73@bk.ru"
        },
        {
            "name": "Dmitry Mamontov",
            "email": "d@mamontov.tech"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": ">=7.1",
      "symfony/config": "^7.3.0",
      "symfony/dependency-injection": "^7.3.0",
      "smi2/phpclickhouse": "^1.4",
      "symfony/console": "^7.3.0",
      "symfony/http-kernel": "^7.3.0"
    },
    "autoload": {
        "psr-4": {
            "Nalogka\\ClickhouseMigrationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    }
}
